@model ClientDetailsViewModel
@{
    ViewData["Title"] = "Details";
}

<div class="card">
  <div class="card-body">
    <h4 class="card-title">Client Form</h4>
    <p class="card-description">Client Details</p>
    <form class="forms-sample">
      <div class="form-group row">
        <label asp-for="Name" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9">
          <input asp-for="Name" type="text" class="form-control-plaintext" id="exampleInputUsername2" readonly>
      </div>
      </div>
      <div class="form-group row">
        <label asp-for = "Phone" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9">
          <input  asp-for = "Phone" type="text" class="form-control-plaintext" id="exampleInputEmail2" readonly>
        </div>  
      </div>
      <div class="form-group row">
        <label asp-for = "Address" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9">
          <input  asp-for = "Address" type="text" class="form-control-plaintext" id="exampleInputEmail2" readonly>
        </div>  
      </div>
      <a asp-action="Index" class="btn btn-gradient-secondary me-2">Go Back</a>
    </form>
  </div>
</div>


